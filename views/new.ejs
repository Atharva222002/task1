<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    <script src="sweetalert2.min.js"></script>
<link rel="stylesheet" href="sweetalert2.min.css">
<style>
    body::before{
    background: url("/images/dmitry-demidko-eBWzFKahEaU-unsplash.jpg") no-repeat center center/cover;
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
</style>
</head>
<body>
    <% if (isAdded) { %>
        <script>
         Swal.fire(
            'Payment Successful',
            'Thank You!!',
            'success'
        )
        function back(){
            document.location='/home';
        }
        setTimeout(back,2000)
       </script>   <% } %>
           <% if (!isAdded) { %>
            <script>
                   Swal.fire({
        icon: 'error',
        title: 'Oops...',
        text: 'Something went wrong!',
      })
            function back(){
                document.location='/home';
            }
            setTimeout(back,2000)
           </script>   <% } %>

</body>
</html>

